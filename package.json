{
  "name": "purpjeopardy",
  "version": "1.1.0",
  "main": "main.js",
"scripts": {
  "start": "cross-env NODE_ENV=development electron .",
  "build": "electron-builder"
},
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^34.1.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.0"
  },
  "build": {
    "appId": "purpjeopardy.game",
    "productName": "PurpJeopardy",
    "directories": {
      "output": "build"
    },
    "files": [
      "main.js",
      "preload.js",
      "package.json",
      "html/**/*",
      "css/**/*",
      "js/**/*",
      "images/**/*",
      "fonts/**/*",
      {
        "from": "boards/exampleBoardData",
        "to": "html/boards/exampleBoardData"
      },
      "!boards"
    ],
    "win": {
      "target": "nsis",
      "icon": "images/icons/game_icon.ico"
    }
  }
}
